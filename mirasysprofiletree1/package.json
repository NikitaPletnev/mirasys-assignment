{
  "name": "mirasysprofiletree",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint ./src --ext .ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf node_modules ios/Pods android/.gradle android/app/build .turbo .cache && yarn install",
    "typecheck": "tsc --noEmit",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:test": "detox test -c android.emu.release",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@react-navigation/bottom-tabs": "^7.4.9",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "graphql": "^16.11.0",
    "react": "18.3.1",
    "react-native": "0.76.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keychain": "^10.0.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "^3.37.0",
    "react-native-svg": "^15.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@mswjs/interceptors": "^0.39.7",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "^0.82.0",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.43.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.11.5",
    "prettier": "^3.6.2",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(svg|png)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-mmkv|react-native-keychain|@react-navigation|react-native-svg)/)"
    ]
  }
}
